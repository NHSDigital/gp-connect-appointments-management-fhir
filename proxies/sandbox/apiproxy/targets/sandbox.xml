<TargetEndpoint name="sandbox">
    <Description/>
    <FaultRules/>
    <Flows/>
    <PostFlow name="PostFlow">
        <Request>
            <Step>
                <Name>AssignMessage.AddAuthorization</Name>
            </Step>
        </Request>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request/>
        <Response>
          <Step>
            <Name>AssignMessage.AddCors</Name>
          </Step>
        </Response>
    </PreFlow>
    <HTTPTargetConnection>
      {{ HOSTED_TARGET_CONNECTION }}
    </HTTPTargetConnection>
</TargetEndpoint>
