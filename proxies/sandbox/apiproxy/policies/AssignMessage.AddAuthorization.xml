<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.AddAuthorization">
    <FaultRules/>
    <Properties/>
    <Set>
        <Headers>
            <Header name="Authorization">Bearer fake-token</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
